  "scripts": {
    "start": "node server.js",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@php-wasm/node": "^0.1.24",
    "@wp-playground/client": "^0.1.25",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "node-fetch": "^3.3.1"
  }
}
